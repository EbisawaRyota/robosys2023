import sys
trendy_words = {
    2000: "モバイル",
    2001: "ドットコム",
    2002: "プラットフォーム",
    2003: "なんでだろ～",
    2004: "チョー気持ちいい",
    2005: "小泉劇場",
    2006: "イナバウアー"
    2007: "どげんかせんといかん",
    2008: "アラフォー",
    2009: "政権交代",
    2010: "ゲゲゲの～",
    2011: "なでしこジャパン",
    2012: "ワイルドだろ～",
    2013: "今でしょ！",
    2014: "ダメよ～ダメダメ",
    2015: "爆買い",
    2016: "神ってる",
    2017: "インスタ映え",
    2018: "そだねー",
    2019: "ONE TEAM(ワンチーム)",
    2020: "3密",
    2021: "リアル二刀流",
    2022: "村神様",
    2023: "アレ",
}

year = int(input("年数を入力してください（2000年から2023年まで）: "))

if year in trendy_words:
    print(f"{year}年の流行語年間大賞は「{trendy_words[year]}」です！")
else:
    print("その年の流行語年間大賞はデータにありません。")
